{
  "build": {
    "production": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "autoIncrement": true,
      "env": {
        "APP_VERSION": "1.0.18",
        "EXPO_NEW_ARCH_ENABLED": "false",
        "REACT_NATIVE_FABRIC_ENABLED": "false",
        "REACT_NATIVE_CONCURRENT_FEATURES": "false"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    }
  },
  "cli": {
    "appVersionSource": "remote"
  }
}
